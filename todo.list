	/* big boy CS50 inspired */
	#include <stdarg.h>
	#include <stdio.h> // For putchar()

	void print(const char *format, ...) {
	    va_list args;
	    va_start(args, format); // 1. Start collecting arguments

	    while (*format) {
		if (*format == '%') {
		    format++; // Look at character after %
		    
		    // 2. Parse the Specifier
		    if (*format == 'd') {
			int n = va_arg(args, int);
			// (Helper function to print digits would go here)
		    } else if (*format == 's') {
			char *s = va_arg(args, char *);
			while (*s) putchar(*s++); // 3. Output Mechanism
		    }
		} else {
		    putchar(*format); // Regular text
		}
		format++;
	    }
	    va_end(args);
	}

